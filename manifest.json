{
"manifest_version": 2,
    "name": "TextReplacer",
    "description": "Chrome extension for text replacement",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "custom.js",
                "replace.js"
            ],
            "run_at": "document_end"
        }
    ]
}